        xmlns:local="clr-namespace:GUI_SF18_2016.GUI"
        mc:Ignorable="d"
        Title="Dodavanje Izmena namestaja" Height="386.099" Width="423.767" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="Naziv: " HorizontalAlignment="Left" Margin="72,37,0,0" VerticalAlignment="Top"/>
        <Label Content="Sifra: " HorizontalAlignment="Left" Margin="78,75,0,0" VerticalAlignment="Top"/>
        <Label Content="Cena:" HorizontalAlignment="Left" Margin="78,113,0,0" VerticalAlignment="Top"/>
